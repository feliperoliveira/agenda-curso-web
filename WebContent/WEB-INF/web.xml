<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Hello</display-name>
  <welcome-file-list>
    <welcome-file>bemvindo.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>primeiro</servlet-name>
    <servlet-class>hello.PrimeiroServlet</servlet-class>
    <init-param>
      <param-name>emailDe</param-name>
      <param-value>suporte@clarify.com.br</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>primeiro</servlet-name>
    <url-pattern>/primeiro</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>compra</servlet-name>
    <servlet-class>hello.Compra</servlet-class>
    <init-param>
      <param-name>emailDe</param-name>
      <param-value>clarify@clarify.com.br</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>compra</servlet-name>
    <url-pattern>/comprar</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>contatos</servlet-name>
    <servlet-class>hello.ListaContatos</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>contatos</servlet-name>
    <url-pattern>/contatos_old</url-pattern>
  </servlet-mapping>
</web-app>